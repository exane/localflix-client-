{
  "name": "localflix-client",
  "version": "0.0.1",
  "description": "",
  "main": "main.js",
  "scripts": {
    "babel": "babel js/app.js --out-file build/app.js --source-maps inline",
    "watch_babel": "babel js/app.js --out-file build/app.js --watch --source-maps inline",
    "reinstall": "rm -rf node_modules && npm i",
    "gulp": "gulp",
    "vlc": "cp -r node_modules/wcjs-prebuilt/bin/* bin&& rm -rf node_modules/webchimera.js&& cd node_modules&& mkdir webchimera.js&& cd ..&& cp -r node_modules/wcjs-prebuilt/* node_modules/webchimera.js",
    "cleanup": "rm -rf bin && mkdir bin && echo '' > bin/.gitkeep && rm -rf node_modules && rm -rf public && mkdir public && echo '' > public/.gitkeep",
    "browserify": "browserify ./js/app.js -o bundle.js -t [ babelify --presets [ es2015 ] ]",
    "compile-win32": "electron-packager . localflix --platform=win32 --arch=ia32",
    "compile-osx": "electron-packager . localflix --platform=darwin --arch=x64",
    "compile": "electron-packager . localflix --all",
    "electron": "electron ."
  },
  "author": "Tim Meier",
  "license": "ISC",
  "dependencies": {
    "angular": "^1.5.3",
    "wcjs-prebuilt": "*",
    "angular-route": "^1.5.5",
    "babel-cli": "^6.7.5",
    "babel-preset-es2015": "^6.6.0",
    "bootstrap": "^3.3.6",
    "electron-packager": "^7.0.1",
    "electron-prebuilt": "0.37.8",
    "gulp": "^3.9.1",
    "gulp-babel": "latest",
    "gulp-concat": "^2.6.0",
    "gulp-sass": "^2.3.1",
    "gulp-sourcemaps": "^2.0.0-alpha",
    "wcjs-player": "^0.5.7"
  }
}
